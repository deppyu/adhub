<tr id="<%=element_id a%>">
  <td><%=link_to a.name,publisher_ad_container_path(a,:format=>:html)%></br><%= a.url%></td>
  <td><%= case a.os_type when Application::OS_ANDROID then 'Android应用' end%></td>
  <td><%= case a.status when Application::STATUS_EDITING then '编辑中'
  	                    when Application::STATUS_ACTIVE then '审核通过'
  	                   when Application::STATUS_LOCKED then '被锁定'
  	                  when Application::STATUS_DISAPPROVED then '提交审核'
  	                  end%></td>
  <td>
  	<div class="dropdown_menu">
     <ul class="menu">
      	<li><%= link_to '编辑',edit_publisher_ad_container_path(a,:format=>:html),:method=>:get%></li>
  	<%if a.status == Application::STATUS_EDITING%>
  	<li> <%= link_to '提交审核',submission_publisher_ad_container_path(a,:format=>:js),:method=>:post,:class=>'remote',:id=>'flag'%></li>
  	<%end%>
     </ul>
     </div>
  </td>
</tr>
